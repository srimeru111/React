//DisplayMessage.js
import React from "react";
function DisplayMessage(props){
return(
<div>
<h2>Message:{props.text}</h2>
</div>
)
}
export default DisplayMessage;
//App.js
import React from "react"
import DisplayMessage from "./DisplayMessage"
function App(){
return(
<div>
<DisplayMessage text="Hello React!"/>
</div>
)
}
export default App;
